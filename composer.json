{
    "name": "webeweb/types-library",
    "type": "library",
    "description": "Types utilities",
    "keywords": [
        "array",
        "boolean",
        "date",
        "float",
        "integer",
        "library",
        "number",
        "object",
        "resource",
        "string",
        "timestamp",
        "types",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/types-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-intl": "*",
        "symfony/yaml": "^2.6|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Types\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Types\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/types-library/issues"
    }
}
